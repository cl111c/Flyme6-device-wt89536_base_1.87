<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="true">
        <bitmap android:src="@drawable/tct_btn_radio_on_disabled_holo" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/tct_btn_radio_off_disabled_holo" />
    </item>
    <item android:state_checked="true" android:id="@id/on">
        <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_015" />
    </item>
    <item android:id="@id/off">
        <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_000" />
    </item>
    <transition android:toId="@id/on" android:fromId="@id/off">
        <animation-list>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_000" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_001" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_002" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_003" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_004" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_005" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_006" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_007" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_008" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_009" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_010" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_011" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_012" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_013" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_014" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_on_mtrl_015" />
            </item>
        </animation-list>
    </transition>
    <transition android:toId="@id/off" android:fromId="@id/on">
        <animation-list>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_off_mtrl_000" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_off_mtrl_001" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_off_mtrl_002" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_off_mtrl_003" />
            </item>
