            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_off_mtrl_012" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_off_mtrl_013" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/tct_btn_radio_to_off_mtrl_014" />
            </item>
            <item android:duration="15">
